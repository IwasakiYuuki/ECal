	.CPU 300HA
	.SECTION A,DATA,LOCATE=H'0FF030
	.DATA.L H'0FF200  ;リセットベクトル
	
	.SECTION P,CODE,LOCATE=H'0FF200
	MOV.L	#H'FFF00,ER7  ;スタックポインタ設定
	MOV.W	@H'FF800:24,R0
	MOV.W	@H'FF802:24,R1
	MOV.W	#0,R2
	JSR		@mymultfunc
	MOV.W	R0,@H'FF804:24
EXL:
	BRA		EXL:8
mymultfunc:
	PUSH.W	R2
L001:
	ADD.W	R0,R2
	DEC.W	#1,R1
	BNE		L001:8
	MOV.W	R2,R0
	POP.W	R2
	RTS
	.END